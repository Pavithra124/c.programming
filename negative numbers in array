//printing negative numbers in an aaray
#include<stdio.h>
int main(){
	int a[1000],i,n;
	printf("enter the size of array:");                                                                        
	scanf("%d",&n);
	printf("enter elements in array");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("\n all negative numbers in an array are:");
	for(i=0;i<n;i++){
		if(a[i]<0){
			printf("%d\t",a[i]);
		}
	}
	return 0;
}
